<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Authors>DavidSciMeow</Authors>
    <Company>Electronicute</Company>
    <Product>MeowIOTBot</Product>
    <Description>这是一个IOTBot开发的SDK
允许您快速构建关于IOTBot的自主封装程序
采用了Analytic-json规范,收发分离,本项目目前注释完全</Description>
    <Copyright>2020 @ Electronicute</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/DavidSciMeow/MeowIOTBot</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageReleaseNotes>修复源文档少许bug,优化了过程流程</PackageReleaseNotes>
    <NeutralLanguage>zh</NeutralLanguage>
    <PackageProjectUrl>https://github.com/DavidSciMeow/MeowIOTBot</PackageProjectUrl>
    <SignAssembly>false</SignAssembly>
    <DelaySign>false</DelaySign>
    <AssemblyOriginatorKeyFile>Electronicute.pfx</AssemblyOriginatorKeyFile>
    <Version>4.0.9</Version>
    <AssemblyVersion>4.0.8.1009</AssemblyVersion>
    <FileVersion>4.0.8.1009</FileVersion>
    <PackageId>Electronicute.MeowIOTBot</PackageId>
    <PackageTags>IOTQQBot, QQBot, Tencent</PackageTags>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>C:\Users\David\source\repos\MeowIOTBot\MeowIOTBot.xml</DocumentationFile>
    <Optimize>false</Optimize>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Electronicute.MeowSocketLibrary" Version="2.0.0" />
    <PackageReference Include="System.Drawing.Common" Version="5.0.0" />
  </ItemGroup>

</Project>
